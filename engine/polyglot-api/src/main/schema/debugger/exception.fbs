namespace org.enso.polyglot.debugger.protocol;

// corresponds to java.lang.StackTraceElement
table StackTraceElement {
  declaringClass: string;
  methodName: string;
  fileName: string;
  lineNumber: int;
}

table ExceptionRepr {
  message: string;
  stackTrace: [StackTraceElement];
  cause: ExceptionRepr;
}
